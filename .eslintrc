{
  "extends": "airbnb",
  "globals": {
    "describe": true,
    "it": true,
    "beforeEach": true,
    "before": true,
    "afterEach": true,
    "after": true
  },
  "rules": {
    "no-var": 0,
    "comma-dangle": [2, "never"],
    "no-space-before-semi": 0,
    "space-after-function-name": 0,
    "semi-spacing": [2, { "before": false, "after": true }],
    "prefer-arrow-callback": 0,
    "no-param-reassign": 0,
    "spaced-line-comment": 0,
    "indent": [2, 2, { "SwitchCase": 1 }]
    }
}

